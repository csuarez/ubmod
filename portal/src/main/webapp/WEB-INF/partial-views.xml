<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    <bean id="defaultToolboxConfig" abstract="true">
        <property name="toolboxConfigLocation" value="/WEB-INF/velocity-toolbox.xml"/>
    </bean>
    
    <bean id="showUserPartial" parent="defaultToolboxConfig" class="org.springframework.web.servlet.view.velocity.VelocityToolboxView">
        <property name="url" value="/views/user/show.vm"/>
    </bean>
    
    <bean id="showGroupPartial" parent="defaultToolboxConfig" class="org.springframework.web.servlet.view.velocity.VelocityToolboxView">
        <property name="url" value="/views/group/show.vm"/>
    </bean>
    
    <bean id="showQueuePartial" parent="defaultToolboxConfig" class="org.springframework.web.servlet.view.velocity.VelocityToolboxView">
        <property name="url" value="/views/queue/show.vm"/>
    </bean>
    
    <bean id="cpuConsumptionPartial" parent="defaultToolboxConfig" class="org.springframework.web.servlet.view.velocity.VelocityToolboxView">
        <property name="url" value="/views/partials/cpu_consumption.vm"/>
    </bean>
    
    <bean id="waitTimePartial" parent="defaultToolboxConfig" class="org.springframework.web.servlet.view.velocity.VelocityToolboxView">
        <property name="url" value="/views/partials/wait_time.vm"/>
    </bean>
    
    <bean id="dashboardPartial" parent="defaultToolboxConfig" class="org.springframework.web.servlet.view.velocity.VelocityToolboxView">
        <property name="url" value="/views/partials/dashboard.vm"/>
    </bean>
    
    <bean id="errorPartial" parent="defaultToolboxConfig" class="org.springframework.web.servlet.view.velocity.VelocityToolboxView">
        <property name="url" value="/views/partials/error.vm"/>
    </bean>
</beans>
