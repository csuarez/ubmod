        { "base": "space_quota",   "type": [ "sum", "max" ] },
        { "base": "inodes_used",  "type": [ "sum", "max" ] },
        { "base": "inodes_available", "type": [ "sum", "max" ] },
        { "base": "inodes_quota", "type": [ "sum", "max" ] }
      ]
    }
