      "dimensions": [
        "dim_date",
        "dim_user",
        "dim_group",
        "dim_tags"
      ],
      "facts": [
        { "base": "space_used", "type": [ "sum", "max" ] },
        { "base": "space_available",  "type": [ "sum", "max" ] },
