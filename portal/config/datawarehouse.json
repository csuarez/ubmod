        { "base": "cput",  "type": [ "sum", "max" ] },
        { "base": "mem",   "type": [ "sum", "max" ] },
        { "base": "vmem",  "type": [ "sum", "max" ] },
        { "base": "wait",  "type": "sum" },
        { "base": "exect", "type": "sum" },
        { "base": "nodes", "type": [ "sum", "max" ] },
        { "base": "cpus",  "type": [ "sum", "max" ] }
      ]
    }, {
      "name": "agg_job_by_timespan",
      "base": "fact_job",
      "dimensions": [
        "dim_timespan",
        "dim_cluster",
        "dim_queue",
        "dim_user",
        "dim_group",
        "dim_tags",
        "dim_cpus"
      ],
      "facts": [
        { "base": "wallt", "type": [ "sum", "max" ] },
        { "base": "cput",  "type": [ "sum", "max" ] },
        { "base": "mem",   "type": [ "sum", "max" ] },
        { "base": "vmem",  "type": [ "sum", "max" ] },
        { "base": "wait",  "type": "sum" },
        { "base": "exect", "type": "sum" },
        { "base": "nodes", "type": [ "sum", "max" ] },
        { "base": "cpus",  "type": [ "sum", "max" ] }
      ]
    },

    {
      "name": "agg_storage_by_all",
